<!--miniprogram/pages/common/msg/error/error.wxml-->
<view class="page">
    <view class="weui-msg">
        <view class="weui-msg__icon-area">
            <icon type="warn" size="93"></icon>
        </view>
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">{{title}}</view>
            <view class="weui-msg__desc">{{info}}</view>
        </view>
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area">
                <button class="weui-btn" bindtap='back'>返回</button>
                <button class="weui-btn" type="default" bindtap='home'>去首页</button>
            </view>
        </view>
    </view>
</view>