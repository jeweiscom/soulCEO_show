/**app.wxss**/
@import 'style/weui/weui.wxss';
page{
    background-color: #F8F8F8;
    font-size: 16px;
    font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}
.page__hd {
    padding: 40px;
}
.page__bd {
    padding-bottom: 40px;
}
.page__bd_spacing {
    padding-left: 15px;
    padding-right: 15px;
}

.page__ft{
    padding-bottom: 10px;
    text-align: center;
}

.page__title {
    text-align: left;
    font-size: 20px;
    font-weight: 400;
}

.page__desc {
    margin-top: 5px;
    color: #888888;
    text-align: left;
    font-size: 14px;
}
.weui-btn{
  background-color: #B22222;
  color: white
}
.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  z-index: 19;
  background-color: rgba(0, 0, 0, 0.33);
  top: 0;
  left: 0;
}